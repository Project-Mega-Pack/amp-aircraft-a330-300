					#DESTINATION_BUS# #SOURCE_BUS_ID# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE)
				}
			</TOGGLE_OFF_CODE>
			<FORCED_OFF_CODE>
